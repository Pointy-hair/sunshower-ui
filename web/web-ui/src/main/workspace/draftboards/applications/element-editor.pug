template
    div(
        show.bind="visible"
        style="position:absolute;  background-color: #F2F2F2; z-index:100; border-color:#D6d6d7 ", ref="element")
        div(style="width:100%; position:relative; height:32px;")
            div(style="float:right; z-index:9000; vertical-align:middle; line-height:32px; padding:4px")
                img(
                    src="assets/sui/themes/hasli/assets/images/icons/x-circle.svg",
                    width="24px",
                    height="24px",
                    click.delegate="close()"
                )
        div(style="width:100%; height:100%; position:relative;margin-top:32px")
            compose(view-model.bind="currentEditor")
