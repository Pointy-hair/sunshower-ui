template
    require(from="form/form")
    form
    .ui.internally.celled.grid
        .row
            .three.wide.fluid.column
                .ui.fluid.text.vertical.menu
                    .item
                        .header Products
                        .menu
                            a.item Enterprise
                            a.item Consumer
                    .item
                        .header CMS Solutions
                        .menu
                            a.item Rails
                            a.item Python
                            a.item PHP
                    .item
                        .header Hosting
